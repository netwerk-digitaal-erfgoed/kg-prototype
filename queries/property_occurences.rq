PREFIX void: <http://rdfs.org/ns/void#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

# Some prefixes are used only in the subject filter (see catalog definitions)
PREFIX schema: <http://schema.org/>

# list properties and their occurences 
CONSTRUCT {
    {{&dataset-uri}} a void:Dataset;
    rdfs:label ?label ;
    void:classPartition [
        void:class ?property;
        void:entities ?total;
    ] .
}
WHERE {
    {{&graph-open}}
    SELECT (COUNT(?property) as ?total) ?property {
        ?subject ?property ?object .
        BIND(CONCAT("totals for property ", STR(?type)) as ?label)
        {{&subject-filter}}
    }
    GROUP BY ?property ?label
    {{&graph-close}}
}
