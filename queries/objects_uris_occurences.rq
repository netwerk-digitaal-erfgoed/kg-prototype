PREFIX void: <http://rdfs.org/ns/void#>

# count objects with URI as value
CONSTRUCT {
    {{&dataset-uri}} a void:Dataset;
    void:distinctObjects ?total .
}
WHERE {
  {{&graph-open}}
  SELECT (COUNT(?object) as ?total) {
    ?subject ?property ?object .
    FILTER(isURI(?object))
    {{&subject-filter}}
  }
  {{&graph-close}}
}
