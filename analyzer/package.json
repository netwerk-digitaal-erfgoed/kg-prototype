    "@oclif/core": "1.x.x",
    "@oclif/plugin-help": "5.x.x",
    "debug": "4.x.x",
    "handlebars": "4.7.7",
    "rdf-data-factory": "1.x.x",
    "rdf-parse": "2.1.0",
    "rdf-serialize": "2.x.x",
    "rdf-store-stream": "1.x.x",
    "stream-to-string": "1.2.0"
  },
  "devDependencies": {
    "@rdfjs/types": "1.1.0",
    "@types/debug": "4.x.x",
    "@types/node": "16.x.x",
    "globby": "11.x.x",
    "gts": "3.x.x",
    "ts-node-dev": "2.x.x",
    "typescript": "4.x.x"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
